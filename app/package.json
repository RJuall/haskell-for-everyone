{
  "name": "haskell-for-everyone",
  "version": "0.1.0",
  "description": "An easy-to-use educational editor for learning the Haskell programming language",
  "author": "test",
  "license": "MIT",
  "email": "test@test",
  "main": "public/electron.js",
  "scripts": {
    "dev": "electron . --dev",
    "dist": "react-scripts build && build",
    "react": "react-scripts start -- --no-browser",
    "start": "electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RJuall/haskell-for-everyone.git"
  },
  "bugs": {
    "url": "https://github.com/RJuall/haskell-for-everyone/issues"
  },
  "homepage": "./",
  "build": {
    "productName": "Haskell For Everyone",
    "win": {
      "icon": "./build/icon.png",
      "target": "NSIS"
    },
    "linux": {
      "icon": "./build/icon.png",
      "target": [
        "AppImage"
      ]
    },
    "dmg": {
      "icon": "./build/icon.png",
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/pro-light-svg-icons": "^5.7.2",
    "@fortawesome/pro-regular-svg-icons": "^5.7.2",
    "@fortawesome/pro-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "babel-preset-mobx": "^2.0.0",
    "bootstrap": "^4.3.1",
    "child_process": "^1.0.2",
    "electron-context-menu": "^0.11.0",
    "events": "^3.0.0",
    "fs": "0.0.1-security",
    "jquery": "^3.3.1",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "mousetrap": "^1.6.3",
    "react": "^16.8.1",
    "react-ace": "^6.4.0",
    "react-dom": "^16.8.2",
    "react-split-pane": "^0.1.85",
    "reactstrap": "^7.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "electron": "^4.1.0",
    "electron-builder": "^20.38.5",
    "electron-react-devtools": "^0.5.3",
    "react-scripts": "^2.1.8"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
